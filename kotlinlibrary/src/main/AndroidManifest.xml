    <application android:allowBackup="true"
                 android:label="@string/app_name"
                 android:supportsRtl="true"
    >
        <activity
            android:name=".KotlinVSJavaActivity"
            android:label="kotlin/KotlinVSJavaActivity" >
            <intent-filter>
                <action android:name="android.intent.action.HAWK_UI" />
                <category android:name="android.intent.category.HAWK_CODE" />
            </intent-filter>
        </activity>
    </application>

</manifest>
